public class Main {
  int modelYear;
  String modelName;

  // Constructor de un parametro
  public Main(String modelName) {
    // Call the two-parameter constructor to reuse code and set a default year    
    this(2020, modelName);
  }

  // Constructor de dos parametros
  public Main(int modelYear, String modelName) {
    // Use 'this' to assign values to the class variables
    this.modelYear = modelYear;
    this.modelName = modelName;
  }

  // Metodo para imprimir la informacion del carro
  public void printInfo() {
    System.out.println(modelYear + " " + modelName);
  }

  public static void main(String[] args) {
    // Crear el coche con el nombre y modelo (uses default year)
    Main car1 = new Main("Corvette");

    // Crear un coche con el a√±o y el modelo
    Main car2 = new Main(1969, "Mustang");

    car1.printInfo();
    car2.printInfo();
  }
}
